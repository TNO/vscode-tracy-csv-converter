{
    "name": "vscode-tracy-csv-converter",
    "displayName": "Tracy CSV converter",
    "version": "0.0.1",
    "publisher": "TNO",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/TNO/vscode-tracy-csv-converter"
    },
    "engines": {
        "vscode": "^1.65.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.tracyCsvConverter",
                "title": "Tracy CSV converter: open current document with Tracy"
            },
            {
                "command": "extension.tracyMultiCsvConverter",
                "title": "Tracy CSV converter: open selected documents with Tracy"
            }
        ]
    },
    "scripts": {
        "package": "vsce package",
        "vscode:prepublish": "npm run compile-extension && npm run compile-viewer -- --mode=production --env production",
        "compile": "npm run compile-extension && npm run compile-viewer -- --mode=development --env development",
        "compile-extension": "tsc -p ./",
        "compile-viewer": "webpack",
        "watch-viewer": "webpack --mode=development --env development --progress --watch"
    },
    "devDependencies": {
        "@types/lodash": "^4.14.198",
        "@types/node": "^16.18.48",
        "@types/papaparse": "^5.3.8",
        "@types/react": "^18.2.21",
        "@types/react-beautiful-dnd": "^13.1.4",
        "@types/react-dom": "^18.2.7",
        "@types/vscode": "^1.65.0",
        "@vscode/codicons": "^0.0.33",
        "@vscode/vsce": "^2.18.0",
        "@vscode/webview-ui-toolkit": "^1.2.2",
        "css-loader": "^6.8.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "style-loader": "^3.3.3",
        "ts-loader": "^9.4.4",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.4",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4"
    },
    "dependencies": {
        "@emotion/react": "^11.11.1",
        "@emotion/styled": "^11.11.0",
        "@mui/material": "^5.14.8",
        "@mui/x-date-pickers": "^6.14.0",
        "dayjs": "^1.11.9",
        "lodash": "^4.17.21",
        "papaparse": "^5.4.1",
        "react-beautiful-dnd": "^13.1.1"
    }
}
